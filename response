   window.loadAdvertAdnetic = function() {
      var advertId2 = "adfox_container_Adnetic";
      var newAdvertId2 = document.getElementById(advertId2);
      newAdvertId2.id = advertId2 + (Math.random() * 100000).toFixed(0);
              getAdfox2(newAdvertId2);   
   };
  
   function getAdfox2(newAdvertId2) {
      var adfoxSlotId_Adnetic = newAdvertId2.id;
      
      window.yaContextCb.push(()=>{
          Ya.adfoxCode.create({
              ownerId: 220463,
              containerId: adfoxSlotId_Adnetic,
              params: {
                  p1: 'coati',
                  p2: 'fqhi'
              }
          })
      })
   }

     window.loadAdvert = function() {
      console.log('infeed loaded');
      var advertId = "adfox_container";
      var newAdvertId = document.getElementById(advertId);
      newAdvertId.id = advertId + (Math.random() * 100000).toFixed(0);
  
      SeedrPlayer({
          container: newAdvertId,
          desiredOffset: 50,
          gid: "5e034b6c55299c29eb6b8914",
          onError: function(e) {
              getAdfox(newAdvertId);
          }
      });
  };
  
  function getAdfox(newAdvertId) {
      var adfoxSlotId = newAdvertId.id;

      window.Ya.headerBidding.pushAdUnits([
          {
              code: adfoxSlotId,
              sizes: [[600, 400],[550, 400]],
              bids: [
              {
                      bidder: "myTarget",
                      params: { placementId: "794491" }
                  },
                  {
                      bidder: "betweenDigital",
                      params: { placementId: "3918325" }
                  },
